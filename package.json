{
  "name": "zw-rec",
  "description": "A library to communicate with an omega zw-rec device",
  "version": "1.0.0",
  "author": "Ryan Albrecht <ryan@albrecht.co.za> ()",
  "bugs": {
    "url": "https://github.com/ryanalbrecht/zw-rec/issues",
    "email": "ryan@albrecht.co.za"
  },
  "contributors": [
    "Ryan Albrecht <ryan@albrecht.co.za> ()"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "deepmerge": "^4.2.2",
    "ping": "^0.2.2",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ava": "latest",
    "codecov": "latest",
    "cross-env": "latest",
    "eslint": "latest",
    "eslint-config-xo-lass": "latest",
    "fixpack": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "nyc": "latest",
    "remark-cli": "latest",
    "remark-preset-github": "latest",
    "xo": "latest"
  },
  "engines": {
    "node": ">=8.3"
  },
  "homepage": "https://github.com/ryanalbrecht/zw-rec",
  "keywords": [
    "zw-rec",
    "omega",
    "thermocouple"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "xo --fix",
      "git add"
    ],
    "*.md": [
      "remark . -qfo",
      "git add"
    ],
    "package.json": [
      "fixpack",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "main": "index.js",
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanalbrecht/zw-rec"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ]
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "xo && remark . -qfo",
    "test": "yarn run lint && yarn run ava",
    "test-coverage": "yarn run lint && yarn run nyc",
    "ava": "cross-env NODE_ENV=test ava",
    "nyc": "cross-env NODE_ENV=test nyc ava"
  }
}
